<%= form_for([@branch, @transfer]) do |f| %>
  <% if @transfer.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@transfer.errors.count, "error") %> prohibited this transfer from being saved:</h2>

      <ul>
      <% @transfer.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= f.label :mode %><br />
    <%= f.text_field :mode %>
  </div>
  <div class="field">
    <%= f.label :name_dealer %><br />
    <!--Aqui me quedeajkxnnnnnnnnnnnnnnnnnnnnnnnnnnnnnnnnn -->
    <%= select(:transfer, :name_dealer, Transfer::DEALER.collect {|cat|, [cat,cat] }, {:include_blank => true}) %>
  </div>
  <div class="field">
    <%= f.label :date_output %><br />
    <%= f.datetime_select :date_output %>
  </div>
  <div class="field">
    <%= f.label :date_supply %><br />
    <%= f.datetime_select :date_supply %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
